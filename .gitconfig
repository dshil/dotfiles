[user]
    name = Dmitriy Shilin
    email = sdadeveloper@gmail.com
[core]
    autocrlf = input
    safecrlf = true
    editor = nvim
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    forkbase = rebase -i --fork-point origin/master
[merge]
    tool = kdiff3
[color]
    ui = true
[rerere]
    enabled = true
